{% extends 'base.html.twig' %}

{% block title %}Adresse{% endblock %}

{% block body %}
    <h1>Adresse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>FullName</th>
                <td>{{ adresse.fullName }}</td>
            </tr>
            <tr>
                <th>NomEntreprise</th>
                <td>{{ adresse.nomEntreprise }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ adresse.address }}</td>
            </tr>
            <tr>
                <th>Complement</th>
                <td>{{ adresse.complement }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ adresse.phone }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ adresse.city }}</td>
            </tr>
            <tr>
                <th>Bodepostal</th>
                <td>{{ adresse.bodepostal }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ adresse.country }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adresse_index') }}">back to list</a>

    <a href="{{ path('adresse_edit', {'id': adresse.id}) }}">edit</a>

    {{ include('adresse/_delete_form.html.twig') }}
{% endblock %}
